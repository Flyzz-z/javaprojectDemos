<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xyz.flyzz.securityjwt.mapper.AuthorityMapper">
    <select id="selectAuthorityIdsByUser" resultType="BigInteger">
        select `authority_id` from  demo2_user_to_authority where `user_id` = #{id}
    </select>
</mapper>